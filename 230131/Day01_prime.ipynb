   "source": [
    "# 소수 : 1과 자기자신으로만 나누어 떨어지는 수. ex) 2, 3, 5, 7...\n",
    "n = 7\n",
    "\n",
    "flag = 0\n",
    "\n",
    "for i in range(2, n):\n",
    "    if n % i == 0:\n",
    "        print('Not prime')\n",
    "        flag = 1\n",
    "        break\n",
    "\n",
    "if flag == 0:\n",
    "    print('prime')"
